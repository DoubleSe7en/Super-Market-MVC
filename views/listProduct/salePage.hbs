<!--- gourmet --->
<div class="products">
	<div class="container">
		<div class="col-md-4 products-left">
			<div class="categories">
				<h2>Danh mục</h2>
				<ul class="cate">
					<li><a href="/products"><i class="fa fa-arrow-right" aria-hidden="true"></i>Trái cây và rau củ</a>
					</li>
					<ul>
						<li><a href="/products"><i class="fa fa-arrow-right" aria-hidden="true"></i>Rau mầm</a></li>
						<li><a href="/products"><i class="fa fa-arrow-right" aria-hidden="true"></i>Hoa</a></li>
						<li><a href="/products"><i class="fa fa-arrow-right" aria-hidden="true"></i>Thảo mộc</a></li>
						<li><a href="/products"><i class="fa fa-arrow-right" aria-hidden="true"></i>Rau</a> </li>
						<li><a href="/products"><i class="fa fa-arrow-right" aria-hidden="true"></i>Rau nhập khẩu</a>
						</li>
						<li><a href="/products"><i class="fa fa-arrow-right" aria-hidden="true"></i>Sản phẩm hữu cơ</a>
						</li>
					</ul>
					<li><a href="/products"><i class="fa fa-arrow-right" aria-hidden="true"></i>Đồ khô</a></li>
					<ul>
						<li><a href="/products"><i class="fa fa-arrow-right" aria-hidden="true"></i>Các loại hạt</a>
						</li>
						<li><a href="/products"><i class="fa fa-arrow-right" aria-hidden="true"></i>Mứt</a> </li>
						<li><a href="/products"><i class="fa fa-arrow-right" aria-hidden="true"></i>Gạo</a> </li>
						<li><a href="/products"><i class="fa fa-arrow-right" aria-hidden="true"></i>Các loại khô</a>
						</li>
					</ul>
					<li><a href="/products"><i class="fa fa-arrow-right" aria-hidden="true"></i>Mẹ và bé</a></li>
					<ul>
						<li><a href="/products"><i class="fa fa-arrow-right" aria-hidden="true"></i>Dầu gội</a> </li>
						<li><a href="/products"><i class="fa fa-arrow-right" aria-hidden="true"></i>Phụ kiện</a> </li>
						<li><a href="/products"><i class="fa fa-arrow-right" aria-hidden="true"></i>Kem dưỡng da</a>
						</li>
						<li><a href="/products"><i class="fa fa-arrow-right" aria-hidden="true"></i>Tã và khăn</a> </li>
					</ul>
				</ul>
			</div>
		</div>
		<div class="col-md-8 products-right">
			<div class="products-right-grid">
				<div class="products-right-grids">
					<div class="sorting">
						<select id="country" onchange="change_country(this.value)" class="frm-field required sect">
							<option value="null"><i class="fa fa-arrow-right" aria-hidden="true"></i>Sắp xếp theo
							</option>
							<option value="null"><i class="fa fa-arrow-right" aria-hidden="true"></i>Sự phổ biến
							</option>
							<option value="null"><i class="fa fa-arrow-right" aria-hidden="true"></i>Giá từ thấp đến cao
							</option>
							<option value="null"><i class="fa fa-arrow-right" aria-hidden="true"></i>Giá từ cao đến thấp
							</option>
						</select>
					</div>
					<div class="sorting-left">
						<select id="country1" onchange="change_country(this.value)" class="frm-field required sect">
							<option value="null"><i class="fa fa-arrow-right" aria-hidden="true"></i>Sản phẩm trên trang
								9</option>
							<option value="null"><i class="fa fa-arrow-right" aria-hidden="true"></i>Sản phẩm trên trang
								18</option>
							<option value="null"><i class="fa fa-arrow-right" aria-hidden="true"></i>Sản phẩm trên trang
								32</option>
							<option value="null"><i class="fa fa-arrow-right" aria-hidden="true"></i>Tất cả</option>
						</select>
					</div>
					<div class="clearfix"> </div>
				</div>
			</div>

			{{#each Product}}
			<div class="col-md-4 agile_top_brands_grids">
				<div class="top_brand_left">
					<div class="hover14 column">
						<div class="agile_top_brand_left_grid">
							<div class="agile_top_brand_left_grid_pos">
								<h4 class="absolute-text">{{this.sale}}%</h4>
								<img src="/images/sale.png" alt=" " class="img-responsive">
							</div>
							<div class="agile_top_brand_left_grid1">
								<figure>
									<div class="snipcart-item block">
										<div class="snipcart-thumb">

											<a href="/products/{{categoryEN this.category}}/{{this._id}}"><img
													class="listProduct" title=" " alt=" " src="{{this.imgSrc}}"></a>


											<p>{{this.name}}</p>
											<h4>{{numberFormat (math this.price "-" (math this.price "*" (math this.sale "/" 100)))}}
												đ
												<span>{{numberFormat this.price}} đ</span>
											</h4>

											

										</div>
										<div class="snipcart-details top_brand_home_details">
											<form action="#" method="post">
												<fieldset>
													<input type="hidden" name="cmd" value="_cart">
													<input type="hidden" name="add" value="1">
													<input type="hidden" name="business" value=" ">
													<input type="hidden" name="item_name" value="Fortune Sunflower Oil">
													<input type="hidden" name="amount" value="35.000 đ">
													<input type="hidden" name="discount_amount" value="1.000">
													<input type="hidden" name="currency_code" value="USD">
													<input type="hidden" name="return" value=" ">
													<input type="hidden" name="cancel_return" value=" ">
													<input type="submit" name="submit" value="Thêm vào giỏ"
														class="button">
												</fieldset>
											</form>
										</div>
									</div>
								</figure>
							</div>
						</div>
					</div>
				</div>
				<div class="clearfix"> </div>
			</div>
			{{/each}}
		</div>
		<div class="clearfix"> </div>
	</div>
	
</div>
<!--- gourmet --->
