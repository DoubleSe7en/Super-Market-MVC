<!--- gourmet --->
<div class="products">
	<div class="container">
		<div class="col-md-4 products-left">
			<div class="categories">
				<h2>Danh mục</h2>
				<ul class="cate">
					<li><a><i class="fa fa-arrow-right" aria-hidden="true"></i>Thực phẩm</a> </li>
					<ul>
						<li><a href="/products/meat"><i class="fa fa-arrow-right" aria-hidden="true"></i>Thịt cá</a></li>
						<li><a href="/products/vegetable"><i class="fa fa-arrow-right" aria-hidden="true"></i>Rau củ</a></li>
						<li><a href="/products/rice"><i class="fa fa-arrow-right" aria-hidden="true"></i>Gạo</a></li>
						<li><a href="/products/dry"><i class="fa fa-arrow-right" aria-hidden="true"></i>Đồ khô</a> </li>
						
					</ul>
					<li><a><i class="fa fa-arrow-right" aria-hidden="true"></i>Đồ gia dụng</a> </li>
					<ul>
						<li><a href="/products/kitchen"><i class="fa fa-arrow-right" aria-hidden="true"></i>Dụng cụ bếp</a></li>
						<li><a href="/products/diningRoom"><i class="fa fa-arrow-right" aria-hidden="true"></i>Phòng ăn</a> </li>
						<li><a href="/products/light"><i class="fa fa-arrow-right" aria-hidden="true"></i>Đèn điện</a> </li>
						<li><a href="/products/cleaning"><i class="fa fa-arrow-right" aria-hidden="true"></i>Vệ sinh nhà cửa</a></li>
					</ul>
					<li><a><i class="fa fa-arrow-right" aria-hidden="true"></i>Em bé</a> </li>
					<ul>
						<li><a href="/products/shampoo"><i class="fa fa-arrow-right" aria-hidden="true"></i>Dầu gội</a> </li>
						<li><a href="/products/utensil"><i class="fa fa-arrow-right" aria-hidden="true"></i>Đồ dùng</a> </li>
						<li><a href="/products/powder"><i class="fa fa-arrow-right" aria-hidden="true"></i>Phấn</a>
						</li>
						<li><a href="/products/milk"><i class="fa fa-arrow-right" aria-hidden="true"></i>Sữa</a> </li>
					</ul>
					<li><a href="/products/gourmet"><i class="fa fa-arrow-right" aria-hidden="true"></i>Ăn vặt</a> </li>
					<li><a href="/products/other"><i class="fa fa-arrow-right" aria-hidden="true"></i>Khác</a> </li>
				</ul>
			</div>
		</div>
		<div class="col-md-8 products-right">
			<div class="products-right-grid">
				<div class="products-right-grids">
					<div class="sorting">
						<select id="country" onchange="change_country(this.value)" class="frm-field required sect">
							<option value="null"><i class="fa fa-arrow-right" aria-hidden="true"></i>Sắp xếp theo
							</option>
							<option value="null"><i class="fa fa-arrow-right" aria-hidden="true"></i>Sự phổ biến
							</option>
							<option value="null"><i class="fa fa-arrow-right" aria-hidden="true"></i>Giá từ thấp đến cao
							</option>
							<option value="null"><i class="fa fa-arrow-right" aria-hidden="true"></i>Giá từ cao đến thấp
							</option>
						</select>
					</div>
					<div class="sorting-left">
						<select id="country1" onchange="change_country(this.value)" class="frm-field required sect">
							<option value="null"><i class="fa fa-arrow-right" aria-hidden="true"></i>Sản phẩm trên trang
								9</option>
							<option value="null"><i class="fa fa-arrow-right" aria-hidden="true"></i>Sản phẩm trên trang
								18</option>
							<option value="null"><i class="fa fa-arrow-right" aria-hidden="true"></i>Sản phẩm trên trang
								32</option>
							<option value="null"><i class="fa fa-arrow-right" aria-hidden="true"></i>Tất cả</option>
						</select>
					</div>
					<div class="clearfix"> </div>
				</div>
			</div>

			{{#each Product}}
			<div class="col-md-4 agile_top_brands_grids">
				<div class="top_brand_left">
					<div class="hover14 column">
						<div class="agile_top_brand_left_grid">
							<div class="agile_top_brand_left_grid_pos">
								{{#isSale this.sale}}
								<h4 class="absolute-text">{{this.sale}}%</h4>
								<img src="/images/sale.png" alt=" " class="img-responsive">
								{{/isSale}}
							</div>
							<div class="agile_top_brand_left_grid1">
								<figure>
									<div class="snipcart-item block">
										<div class="snipcart-thumb">

											<a href="/products/{{categoryEN this.category}}/{{this._id}}"><img
													class="listProduct" title=" " alt=" " src="{{this.imgSrc}}"></a>


											<p>{{this.name}}</p>
											<div class="stars">
														<i class="fa fa-star blue-star" aria-hidden="true"></i>
														<i class="fa fa-star blue-star" aria-hidden="true"></i>
														<i class="fa fa-star blue-star" aria-hidden="true"></i>
														<i class="fa fa-star blue-star" aria-hidden="true"></i>
														<i class="fa fa-star gray-star" aria-hidden="true"></i>
													</div>
											{{#isSale this.sale}}
											<h4>{{getPrice this.price this.sale}} đ <span>{{numberFormat this.price}} đ</span></h4>

											{{/isSale}}
											{{#isNotSale this.sale}}
											<h4>{{numberFormat this.price}} đ</h4>
											{{/isNotSale}}

										</div>
										<div class="snipcart-details top_brand_home_details">
											<form action="#" method="post">
												<fieldset>
													<input type="hidden" name="cmd" value="_cart">
													<input type="hidden" name="add" value="1">
													<input type="hidden" name="business" value=" ">
													<input type="hidden" name="item_name" value="Fortune Sunflower Oil">
													<input type="hidden" name="amount" value="35.000 đ">
													<input type="hidden" name="discount_amount" value="1.000">
													<input type="hidden" name="currency_code" value="USD">
													<input type="hidden" name="return" value=" ">
													<input type="hidden" name="cancel_return" value=" ">
													<input type="submit" name="submit" value="Thêm vào giỏ"
														class="button">
												</fieldset>
											</form>
										</div>
									</div>
								</figure>
							</div>
						</div>
					</div>
				</div>
				<div class="clearfix"> </div>
			</div>
			{{/each}}


			{{#ifCond count "<=" 0}}
			<h4>Trang cuối</h4>
			{{/ifCond}}
		</div>
		<div class="clearfix"> </div>
	</div>
	<nav class="numbering">
		<ul class="pagination paging">
			<li>
				<a id="prev" aria-label="Previous">
					<span aria-hidden="true">&laquo;</span>
				</a>
			</li>
			<li class="active"><a id="page1">{{page}}</a></li>
			<li><a id="page2"></a></li>
			<li><a id="page3"></a></li>
			<li>
				<a id="next" aria-label="Next">
					<span aria-hidden="true">&raquo;</span>
				</a>
			</li>
		</ul>
	</nav>
</div>
<!--- gourmet --->
<script>
	var page1 = parseInt($('#page1').html());
    var page2 = page1 + 1;
    var page3 = page1 + 2;
	$('#page2').click(function () {
		if ({{count}} == 2) {
			$('#page2').attr("href", "/products/{{categoryEnglish}}/viewPage/" + page2);
			$('#page1').removeClass('active');
			$('#page2').addClass('active');
			$('#page3').removeClass('active');
			$('#page1').html(page1);
			$('#page2').html(page2);
			$('#page3').html(page3);
			console.log(page3);
		}
	});
	$('#page3').click(function () {
		if ({{count}} == 2) {
			$('#page3').attr("href", "/products/{{categoryEnglish}}/viewPage/" + page3);
			$('#page1').removeClass('active');
			$('#page2').removeClass('active');
			$('#page3').addClass('active');
			$('#page1').html(page1);
			$('#page2').html(page2);
			$('#page3').html(page3);
		}
	});
	$('#next').click(function () {
		if ({{count}} == 2) {
			page1 += 1;
			page2 += 1;
			page3 += 1;
			$('#next').attr("href", "/products/{{categoryEnglish}}/viewPage/" + page1);
			$('#page1').addClass('active');
			$('#page2').removeClass('active');
			$('#page3').removeClass('active');
			$('#page1').html(page1);
			$('#page2').html(page2);
			$('#page3').html(page3);
		}
	});
	$('#prev').click(function () {
		if (page1 > 1) {
			page1 -= 1;
			page2 -= 1;
			page3 -= 1;
			$('#prev').attr("href", "/products/{{categoryEnglish}}/viewPage/" + page1);
			$('#page1').addClass('active');
			$('#page2').removeClass('active');
			$('#page3').removeClass('active');
			$('#page1').html(page1);
			$('#page2').html(page2);
			$('#page3').html(page3);
		}
	});
	$('#page2').html(page2);
	$('#page3').html(page3);
</script>