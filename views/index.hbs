<!-- top-brands -->
<div class="top-brands">
	<div class="container">
		<h2>Những sản phẩm bán chạy</h2>
		<div class="grid_3 grid_5">
			<div class="bs-example bs-example-tabs" role="tabpanel" data-example-id="togglable-tabs">

				<div id="myTabContent" class="tab-content">
					{{#each Product}}
					<div class="col-md-3 agile_top_brands_grids">
						<div class="top_brand_left">
							<div class="hover14 column">
								<div class="agile_top_brand_left_grid">
									<div class="agile_top_brand_left_grid_pos">
										{{#isSale this.sale}}
										<h4 class="absolute-text">{{this.sale}}%</h4>
										<img src="/images/sale.png" alt=" " class="img-responsive">
										{{/isSale}}
									</div>
									<div class="agile_top_brand_left_grid1">
										<figure>
											<div class="snipcart-item block">
												<div class="snipcart-thumb">

													<a href="/products/{{categoryEN this.category}}/{{this._id}}"><img
															class="listProduct" title=" " alt=" "
															src="{{this.imgSrc}}"></a>


													<p>{{this.name}}</p>
													<div class="stars">
														<i class="fa fa-star blue-star" aria-hidden="true"></i>
														<i class="fa fa-star blue-star" aria-hidden="true"></i>
														<i class="fa fa-star blue-star" aria-hidden="true"></i>
														<i class="fa fa-star blue-star" aria-hidden="true"></i>
														<i class="fa fa-star gray-star" aria-hidden="true"></i>
													</div>
													{{#isSale this.sale}}
													<h4>{{numberFormat (math this.price "-" (math this.price "*" (math this.sale "/" 100)))}}
														đ
														<span>{{numberFormat this.price}} đ</span>
													</h4>

													{{/isSale}}
													{{#isNotSale this.sale}}
													<h4>{{numberFormat this.price}} đ</h4>
													{{/isNotSale}}

												</div>
												<div class="snipcart-details top_brand_home_details">
													<form action="#" method="post">
														<fieldset>
															<input type="hidden" name="cmd" value="_cart">
															<input type="hidden" name="add" value="1">
															<input type="hidden" name="business" value=" ">
															<input type="hidden" name="item_name"
																value="Fortune Sunflower Oil">
															<input type="hidden" name="amount" value="35.000 đ">
															<input type="hidden" name="discount_amount" value="1.000">
															<input type="hidden" name="currency_code" value="USD">
															<input type="hidden" name="return" value=" ">
															<input type="hidden" name="cancel_return" value=" ">
															<input type="submit" name="submit" value="Thêm vào giỏ"
																class="button">
														</fieldset>
													</form>
												</div>
											</div>
										</figure>
									</div>
								</div>
							</div>
						</div>
						<div class="clearfix"> </div>
					</div>
					{{/each}}


					<div class="clearfix"> </div>

				</div>
			</div>
		</div>
	</div>
</div>
